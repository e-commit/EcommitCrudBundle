{
    "name": "ecommit/crud-bundle",
    "description": "EcommitCrudBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
