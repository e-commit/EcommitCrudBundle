<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ecommit_crud.filter.boolean" class="Ecommit\CrudBundle\Form\Filter\BooleanFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.choice" class="Ecommit\CrudBundle\Form\Filter\ChoiceFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.date" class="Ecommit\CrudBundle\Form\Filter\DateFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.entity_ajax" class="Ecommit\CrudBundle\Form\Filter\EntityAjaxFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.entity" class="Ecommit\CrudBundle\Form\Filter\EntityFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.integer" class="Ecommit\CrudBundle\Form\Filter\IntegerFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.not_null" class="Ecommit\CrudBundle\Form\Filter\NotNullFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.null" class="Ecommit\CrudBundle\Form\Filter\NullFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.number" class="Ecommit\CrudBundle\Form\Filter\NumberFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="ecommit_crud.filter.text" class="Ecommit\CrudBundle\Form\Filter\TextFilter">
            <tag name="ecommit_crud.filter" />
        </service>
    </services>
</container>
