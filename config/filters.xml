            <tag name="ecommit_crud.filter" />
        </service>

        <service id="Ecommit\CrudBundle\Form\Filter\ChoiceFilter" class="Ecommit\CrudBundle\Form\Filter\ChoiceFilter">
            <tag name="ecommit_crud.filter" />
        </service>

        <service id="Ecommit\CrudBundle\Form\Filter\DateFilter" class="Ecommit\CrudBundle\Form\Filter\DateFilter">
